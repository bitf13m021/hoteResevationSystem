@model List< Hotel_Reservation_System.Models.User_booking>
@{
    ViewBag.Title = "Data";
}

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script src="~/Scripts/jquery.js"></script>
<script type="text/javascript">
    $(function () {


        $(".btn_delete").click(function () {

            //Save the link in a variable called element
            var element = $(this);

            //Find the id of the link that was clicked
            var del_id = element.attr("Id");

            //Built a url to send
            var Id = 'id=' + del_id;
            if (confirm("Sure you want to delete this update? There is NO undo!")) {

                $.ajax({
                    type: "GET",
                    url: "@Url.Action("deleteData", "Admin")",
                    data: Id,
                    success: function () {
                        alert("Data deleted successfully");

                    }
                });
                $(this).parents(".record").animate({ backgroundColor: "#fbc7c7" }, "fast")
               .animate({ opacity: "hide" }, "slow");

            }



        });
        return false;

    });
</script>
</script>
<section id="main-content">
    <section class="wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h3 class="page-header"><i class="fa fa-table"></i> DataBase</h3>
                <ol class="breadcrumb">
                    <li><i class="fa fa-home"></i><a href="index.html">Home</a></li>
                    <li><i class="fa fa-table"></i>DataBase</li>
                    <li><i class="fa fa-th-list"></i>user booking</li>
                </ol>
            </div>
        </div>
        <!-- page start-->
        <div class="row">
            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        Room Details
                    </header>
                    <table class="table">
                        <thead>
                            <tr>
                                   <th>User Name</th>
                                <th>Email</th>
                                <th>Room Name</th>
                                <th>Check In Date </th>
                                <th>Check In Date</th>
                                     </tr>
                        </thead>
                        <tbody>
                            @foreach (var m in Model) { 
                            <tr>
                                   <td>@m.user_Name </td>
                                <td>@m.Email</td>
                                <td>@m.romm_name</td>
                                <td>@m.Idate</td>
                                <td>@m.odate</td>
                            </tr>}
                    </table>


                </section>
            </div>
           

                        
    </section>
    </div>
    </div>
    <!-- page end-->
</section>
</section>
<!--main content end-->
  </section>
<!-- container section end -->
<!-- javascripts -->
<script src="~/Scripts/Admin/jquery.js"></script>
<script src="~/Scripts/Admin/bootstrap.min.js"></script>
